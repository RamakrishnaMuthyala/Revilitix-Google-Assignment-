{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-np-testpractice-cmn"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"ls_SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_SqlServer1'"
		},
		"ls_adls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_datalake'"
		},
		"ls_adls_datalake2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_datalake2'"
		},
		"ls_SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-QI2VJ85\\SQLEXPRESS"
		},
		"ls_SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DB_Sample"
		},
		"ls_SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"ls_adls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sanpindiatestpractice.dfs.core.windows.net/"
		},
		"ls_adls_datalake2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stdatalakenpcin.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-np-testpractice-cmn_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"ls_adls_datalake_accountKey": {
						"value": "[parameters('ls_adls_datalake_accountKey')]"
					},
					"ls_adls_datalake2_accountKey": {
						"value": "[parameters('ls_adls_datalake2_accountKey')]"
					},
					"ls_SqlServer1_password": {
						"value": "[parameters('ls_SqlServer1_password')]"
					},
					"ls_adls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_datalake_properties_typeProperties_url')]"
					},
					"ls_adls_datalake2_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_datalake2_properties_typeProperties_url')]"
					},
					"ls_SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('ls_SqlServer1_properties_typeProperties_server')]"
					},
					"ls_SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('ls_SqlServer1_properties_typeProperties_database')]"
					},
					"ls_SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('ls_SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}